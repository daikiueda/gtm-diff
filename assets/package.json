  },
  "homepage": "https://github.com/daikiueda/gtm-diff",
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "babelify": "^7.3.0",
    "bower": "^1.7.9",
    "browser-sync": "^2.13.0",
    "browserify": "^13.0.1",
    "browserify-shim": "^3.8.12",
    "cross-env": "^1.0.8",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-gh-pages": "^0.5.3",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.1.1",
