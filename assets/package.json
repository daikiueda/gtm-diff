    "lodash.union": "^4.4.0",
    "postcss-import": "^8.1.2",
    "q": "^1.4.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.1",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "run-sequence": "^1.2.1",
